{
  "name": "College Email Filter",
  "version": "0.1.1",
  "description": "This extension moves emails from specified schools out of your inbox",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Basic Info"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
        "tabs",
        "storage",
        "identity",
        "https://*.google.com/*",
        "https://ssl.gstatic.com/",
        "https://www.googleapis.com/",
        "http://collegefilter.azurewebsites.net/",
        "https://accounts.google.com/"
    ],
  "oauth2": {
        "client_id": "478849294151-rbla73lpg115hoq3aqb7dildkf7b7tge.apps.googleusercontent.com",
        "scopes": [
            "https://mail.google.com/",
            "https://www.googleapis.com/auth/gmail.readonly",
            "https://www.googleapis.com/auth/gmail.compose",
            "https://www.googleapis.com/auth/gmail.labels",
            "https://www.googleapis.com/auth/gmail.modify"
        ]
  }
}